package sde;

import java.awt.*;
import java.awt.geom.*;



public class State extends DiagramElement{
	int width;
	int height;
	
	@Override
	public void paint(Graphics g) {
		Graphics2D g2d = (Graphics2D) g;
        g2d.draw(new Ellipse2D.Double(20,20,20,20));
	}
	@Override
	public boolean intersect(Point p) {
		return true;	
	}
	
}